{
  "title": "Autopark — SLI (v3.1, fallbacks)",
  "schemaVersion": 39,
  "editable": true,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "RPS (total)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket[5m])))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p99 (s)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket[5m])))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Error rate 5xx (%)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "100 * ( (sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])) or vector(0)) / clamp_min(sum(rate(http_server_requests_seconds_count[5m])), 1e-9) )",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Tomcat busy / max (%)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "100 * ( (avg(tomcat_threads_busy) or avg(tomcat_threads_busy_threads) or vector(0)) / clamp_min( (max(tomcat_threads_config_max) or max(tomcat_threads_config_max_threads)), 1) )",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Hikari pending (waiters)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "max by (pool) (hikaricp_connections_pending)",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Hikari acquire p95 (s, fallback→mean)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le,pool) (rate(hikaricp_connections_acquire_seconds_bucket[5m]))) OR ( sum by (pool) (rate(hikaricp_connections_acquire_seconds_sum[5m])) / clamp_min(sum by (pool) (rate(hikaricp_connections_acquire_seconds_count[5m])), 1e-9) )",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "GC pause/sec (s/s)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_sum[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "hidden"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s/s"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Top-5 RPS by URI",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "expr": "topk(5, sum by (uri) (rate(http_server_requests_seconds_count[5m])))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Top-5 p95 by URI (s)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "targets": [
        {
          "expr": "topk(5, histogram_quantile(0.95, sum by (le,uri) (rate(http_server_requests_seconds_bucket[5m]))))",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    }
  ]
}
